{
    "header": {
        "buttons" : {
            "user-locate": {
                "class": "user-locate",
                "data-role": "button",
                "data-inline": "true",
                "data-transition": "none",
                "data-iconpos": "notext"
            }
        }
    },
    "body": {
        "save-map-buttons": {
            "item1": {
                "div": {"class": "ui-block-a", "style": "width:50%"}
            },
            "item2": {
                "div": {"class": "ui-block-b", "id": "save-map-buttons-ok", "style": "width:50%"},
                "a": {"data-role": "button"},
                "title": "Download Map",
                "i18n": "offline-maps:download_map_title"
            }
        },
        "list-download-map": {
            "i18n": "offline-maps:list_downloaded_maps",
            "default": "List downloaded maps"
        },
        "download-map": {
            "i18n": "offline-maps:download_map",
            "default": "Download Map"
        },
        "save-map": {
            "i18n": "offline-maps:save_map",
            "default": "Save Map (use offline)"
        },
        "save": {
            "i18n": "offline-maps:save",
            "default": "Save"
        },
        "zoom": {
            "i18n": "offline-maps:zoom_levels",
            "default": "Zoom Levels"
        }
    },
    "popups": {
        "save":{
            "template": "save-map-name-popup.html",
            "data":{
                "title": {
                    "i18n": "offline-maps:saved_map_name",
                    "default": "Saved Map Name"
                },
                "text": {
                    "i18n": "offline-maps:enter_map_name",
                    "default": "Enter map name"
                },
                "save": {
                    "i18n": "offline-maps:save",
                    "default": "Save"
                },
                "cancel": {
                    "i18n": "offline-maps:cancel",
                    "default": "Cancel"
                }
            }
        }
    }
}
